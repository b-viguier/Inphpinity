#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

$renderEngine = new \Inphpinity\Infrastructure\Render\SDL2\Engine();

$gridLoader = require __DIR__.'/../config/grids/bootstrap.php';
